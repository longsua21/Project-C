#include <stdio.h>

int main() {
    int arr[10];  // Khai báo mảng có 10 phần tử

    // Nhập giá trị cho mảng
    printf("Nhap 10 so nguyen:\n");
    for (int i = 0; i < 10; i++) {
        printf("Phan tu arr[%d] = ", i);
        scanf("%d", &arr[i]);
    }

    // In mảng theo hàng ngang
    printf("Mang vua nhap la:\n");
    for (int i = 0; i < 10; i++) {
        printf("%d ", arr[i]);
    }

    printf("\n");
    return 0;
}
